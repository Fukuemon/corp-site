{
  "specs": {
    "\\.[jt]sx?$": "@markuplint/react-spec"
  },
  "parser": {
    "\\.[jt]sx?$": "@markuplint/jsx-parser"
  },
  "extends": [
    "markuplint:recommended-react"
  ],
  "overrides": {
    "src/stories/*": {
      "rules": {
        "any-rule": false
      }
    },
    "src/components/ui/*": {
      "rules": {
        "any-rule": false
      }
    }
  }
}
